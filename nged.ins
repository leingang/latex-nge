% Here is where we make a a config file based on subversion info!
\documentclass{minimal}
\usepackage{svninfo}
\svnInfo $Id: nged.dtx 516 2009-08-28 20:11:08Z leingang $
\svnKeyword $HeadURL: https://subversive.cims.nyu.edu/mathclinical/nged/branches/3.0-dev1/nged.dtx $
\edef\filedate{\svnInfoMaxYear/\svnInfoMaxMonth/\svnInfoMaxDay}
% parse the URL to get the branch/tag/trunk
\makeatletter
\def\svnInfoBranch{--branch--}
\def\svnInfoTag{--tag--}
\newif\ifsvnInfoIsBranch\svnInfoIsBranchfalse
\newif\ifsvnInfoIsTag\svnInfoIsTagfalse
\newif\ifsvnInfoIsTrunk\svnInfoIsTrunkfalse
\def\@parseSvnInfoHeadURL@branchflag{branches}
\def\@parseSvnInfoHeadURL@tagflag{tags}
\def\@parseSvnInfoHeadURL@trunkflag{trunk}
\def\@parseSvnInfoHeadURL@branch#1/#2/\relax/{\svnInfoIsBranchtrue\xdef\svnInfoBranch{#1}}
\def\@parseSvnInfoHeadURL@tag#1/#2/\relax/{\svnInfoIsTagtrue\xdef\svnInfoTag{#1}}
\def\@parseSvnInfoHeadURL@trunk#1/\relax/{\svnInfoIsTrunktrue}
\def\parseSvnInfoHeadURL{\expandafter\@parseSvnInfoHeadURL\svnInfoHeadURL/\relax/}
\def\@parseSvnInfoHeadURL#1/{
	\let\@parseSvnInfoHeadURL@next=\relax
	\ifx\relax#1
		\let\@parsesvnInfoHeadURL@next=\empty%
	\else
		\ifthenelse{\equal{#1}{\@parseSvnInfoHeadURL@branchflag}}
			{\let\@parseSvnInfoHeadURL@next=\@parseSvnInfoHeadURL@branch}
		    {\ifthenelse{\equal{#1}{\@parseSvnInfoHeadURL@tagflag}}
				{\expandafter\@parseSvnInfoHeadURL@tag}
				{\ifthenelse{\equal{#1}{\@parseSvnInfoHeadURL@trunkflag}}
					{\let\@parseSvnInfoHeadURL@next=\@parseSvnInfoHeadURL@trunk}
					{\let\@parseSvnInfoHeadURL@next=\@parseSvnInfoHeadURL}}}
	\fi
	\expandafter\@parseSvnInfoHeadURL@next
}
\parseSvnInfoHeadURL
\ifsvnInfoIsBranch
	\xdef\fileversion{b\svnInfoBranch}
\fi
\ifsvnInfoIsTag
	\xdef\fileversion{v\svnInfoTag}
\fi
\makeatother

\newwrite\configfile
\immediate\openout\configfile=nged.def
\immediate\write\configfile{\string\def\string\filedate{\filedate}}
\immediate\write\configfile{\string\def\string\fileversion{\fileversion}}
\immediate\write\configfile{\string\def\string\svnInfoBranch{\svnInfoBranch}}
\immediate\write\configfile{\string\def\string\svnInfoTag{\svnInfoTag}}
\immediate\closeout\configfile
\begin{document}
\end{document}

\endbatchfile
